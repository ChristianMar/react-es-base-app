{
  "name": "upgraded",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "--- INSTALL ---": "install all dependency with yarn install-all-dependency",
    "remove-root-node-modules": "rm -rf ./node_modules || true",
    "remove-webapp-node-modules": "rm -rf ./source/app/webapp/node_modules || true",
    "remove-main-node-modules": "rm -rf ./source/app/main/node_modules || true",
    "remove-ui-node-modules": "rm -rf ./source/app/ui/node_modules || true",
    "remove-all-node-modules": "yarn remove-root-node-modules && yarn remove-webapp-node-modules && yarn remove-main-node-modules && yarn remove-ui-node-modules",
    "install-root-node-modules": "yarn",
    "install-webapp-node-modules": "cd ./source/app/webapp/ && yarn",
    "install-main-node-modules": "cd ./source/app/main/ && yarn",
    "install-ui-node-modules": "cd ./source/app/ui/ && yarn",
    "cleanup": "yarn remove-all-node-modules && yarn install-root-node-modules && yarn install-webapp-node-modules && yarn install-main-node-modules && yarn install-ui-node-modules",
    "--- RUN DEV ---": "run dev app with yarn electron-dev",
    "start-dev": "webpack-dev-server --config ./source/webpack/webpack.dev.js --mode development --env dev",
    "--- FIX PROD FOLDER ---": "used by build",
    "remove-build-folder": "rm -rf build || true",
    "--- BUILD DEV ---": "build dev and release app",
    "build-dev": "yarn remove-build-folder && webpack --config ./source/webpack/webpack.prod.js --mode production --env dev",
    "--- BUILD UAT ---": "build uat and release app",
    "build-uat": "yarn remove-build-folder && webpack --config ./source/webpack/webpack.prod.js --mode production --env uat",
    "--- BUILD PROD ---": "build prod and release app",
    "build-prod": "yarn remove-build-folder && webpack --config ./source/webpack/webpack.prod.js --mode production --env prod"
  },
  "resolutions": {
    "**/ua-parser-js": "0.7.28"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "async-mutex": "^0.3.2",
    "axios": "^1.4.0",
    "final-form": "^4.20.6",
    "node-polyglot": "^2.4.2",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-final-form": "^6.5.8",
    "react-polyglot": "^0.7.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  }
}
